<Page x:Class="Launching_Interface.StartPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launching_Interface"
        mc:Ignorable="d"
         d:DesignHeight="600" d:DesignWidth="800"
         Title="StartPage">
    <Grid>
        <Image Source="Pictures/chateau.png" Stretch="Fill" VerticalAlignment="Stretch" Margin="0"/>
        <Grid Name="DisplaySP" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Transparent">
            
        

            <Viewbox Stretch="Fill" Width="auto" Height="auto">
                <TextBox Text="Hyper" BorderThickness="0" Margin="10,10,10,10" Background="Transparent" Foreground="White" BorderBrush="Transparent" FontWeight="SemiBold" HorizontalAlignment="Stretch" Width="auto" VerticalAlignment="Stretch" Opacity="0.9"/>
            </Viewbox>
            <Button Name="Principal" Background="Transparent"  Width="auto" Height="auto" Foreground="White" VerticalAlignment="Stretch" BorderThickness="0" HorizontalAlignment="Stretch" Click="Principal_Click" Margin="0"/>


        </Grid>
    </Grid>
</Page>
